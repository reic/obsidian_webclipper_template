{
	"schemaVersion": "0.1.0",
	"name": "鏡報",
	"behavior": "create",
	"noteContentFormat": "# {{title}}\n{{selectorHtml:article.brief|markdown}}\n\n{{selectorHtml:article.content|markdown}}\n\n\n# 資料來源\n{{meta:name:article:published_time|date:\"YYYY/MM/DD\"}}: [{{title}}]({{url}}) ",
	"properties": [
		{
			"name": "新聞來源",
			"value": "{{site}}",
			"type": "text"
		},
		{
			"name": "published",
			"value": "{{meta:name:article:published_time|date:\\\"YYYY-MM-DD\\\"}}",
			"type": "date"
		}
	],
	"triggers": [
		"https://www.mirrordaily.news/"
	],
	"noteNameFormat": "{{title}}",
	"path": "0 Fleet Note"
}