{
	"schemaVersion": "0.1.0",
	"name": "太報",
	"behavior": "create",
	"noteContentFormat": "# {{title}}\n{{selectorHtml: div.news-box-text|remove_html:(.news-label) |markdown}}\n\n# 資料來源\n{{published|date:\"YYYY/MM/DD\"}}: [{{title}}]({{url}}) ",
	"properties": [
		{
			"name": "新聞來源",
			"value": "{{site}}",
			"type": "text"
		},
		{
			"name": "published",
			"value": "{{published|split:\\\" \\\"|first}}",
			"type": "date"
		}
	],
	"triggers": [
		"https://www.taisounds.com/"
	],
	"noteNameFormat": "{{title}}",
	"path": "0 Fleet Note"
}