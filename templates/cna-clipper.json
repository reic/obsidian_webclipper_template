{
	"schemaVersion": "0.1.0",
	"name": "CNA",
	"behavior": "create",
	"noteContentFormat": "# {{title}}\n{{selector:div.paragraph>p|slice:0,-1|join:\"\\n\\n\"}}\n\n# 資料來源\n{{published|date:\"YYYY/MM/DD\"}}: [{{title}}]({{url}}) ",
	"properties": [
		{
			"name": "新聞來源",
			"value": "{{meta:property:og:site_name}}",
			"type": "text"
		}
	],
	"triggers": [
		"https://www.cna.com.tw/"
	],
	"noteNameFormat": "{{title}}",
	"path": "0 Fleet Note"
}