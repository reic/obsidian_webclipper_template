{
	"schemaVersion": "0.1.0",
	"name": "工商時報",
	"behavior": "create",
	"noteContentFormat": "# {{title}}\n{{selectorHtml:article|remove_html:.related-inline|markdown}}\n\n# 資料來源\n{{meta:name:pubdate|date:\"YYYY/MM/DD\"}}: [{{title}}]({{url}}) ",
	"properties": [
		{
			"name": "新聞來源",
			"value": "{{meta:property:og:site_name}}",
			"type": "text"
		},
		{
			"name": "published",
			"value": "{{meta:name:pubdate|date:\\\"YYYY-MM-DD\\\"}}",
			"type": "date"
		}
	],
	"triggers": [
		"https://www.ctee.com.tw/"
	],
	"noteNameFormat": "{{title}}",
	"path": "0 Fleet Note"
}