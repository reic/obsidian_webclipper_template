{
	"schemaVersion": "0.1.0",
	"name": "經濟日報",
	"behavior": "create",
	"noteContentFormat": "# {{title}}\n{{selectorHtml:figure.article-image|selectorHtml:img|markdown}}\n{{selectorHtml:section#article_body|markdown}}{{selector:#main > p:nth-child(n)|join:\"\\n\"|trim}}\n\n# 資料來源\n{{date|date:\"YYYY/MM/DD\"}}: [{{title}}]({{url}}) ",
	"properties": [
		{
			"name": "新聞來源",
			"value": "{{meta:property:og:site_name}}",
			"type": "text"
		}
	],
	"triggers": [
		"https://money.udn.com/"
	],
	"noteNameFormat": "{{title}}",
	"path": "0 Fleet Note"
}