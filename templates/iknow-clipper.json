{
	"schemaVersion": "0.1.0",
	"name": "iknow",
	"behavior": "create",
	"noteContentFormat": "# {{title}}\n{{selectorHtml:div#data_content|remove_html:(.post-share-content)|markdown}}\n\n# 資料來源\n{{published|date:\"YYYY/MM/DD\"}}: [{{title}}]({{url}}) ",
	"properties": [
		{
			"name": "新聞來源",
			"value": "{{site}}",
			"type": "text"
		},
		{
			"name": "published",
			"value": "{{published|date:\\\"YYYY-MM-DD\\\"}}",
			"type": "date"
		}
	],
	"triggers": [
		"https://iknow.stpi.niar.org.tw/"
	],
	"noteNameFormat": "{{title}}",
	"path": "0 Fleet Note"
}